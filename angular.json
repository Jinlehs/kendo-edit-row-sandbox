{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-kendo-unite": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/angular-kendo-unite",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              {
                "input": "node_modules/@progress/kendo-theme-default/dist/all.css"
              },
              "src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "angular-kendo-unite:build:production"
            },
            "development": {
              "browserTarget": "angular-kendo-unite:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-kendo-unite:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              {
                "input": "node_modules/@progress/kendo-theme-default/dist/all.css"
              },
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "@framewerx/forms": {
      "projectType": "library",
      "root": "projects/framewerx/forms",
      "sourceRoot": "projects/framewerx/forms/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/framewerx/forms/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/framewerx/forms/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/framewerx/forms/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/framewerx/forms/src/test.ts",
            "tsConfig": "projects/framewerx/forms/tsconfig.spec.json",
            "karmaConfig": "projects/framewerx/forms/karma.conf.js"
          }
        }
      }
    },
    "@framewerx/core": {
      "projectType": "library",
      "root": "projects/framewerx/core",
      "sourceRoot": "projects/framewerx/core/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/framewerx/core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/framewerx/core/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/framewerx/core/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/framewerx/core/src/test.ts",
            "tsConfig": "projects/framewerx/core/tsconfig.spec.json",
            "karmaConfig": "projects/framewerx/core/karma.conf.js"
          }
        }
      }
    },
    "@framewerx/kendo-angular-grid": {
      "projectType": "library",
      "root": "projects/framewerx/kendo-angular-grid",
      "sourceRoot": "projects/framewerx/kendo-angular-grid/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/framewerx/kendo-angular-grid/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/framewerx/kendo-angular-grid/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/framewerx/kendo-angular-grid/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/framewerx/kendo-angular-grid/src/test.ts",
            "tsConfig": "projects/framewerx/kendo-angular-grid/tsconfig.spec.json",
            "karmaConfig": "projects/framewerx/kendo-angular-grid/karma.conf.js"
          }
        }
      }
    }
  }
}